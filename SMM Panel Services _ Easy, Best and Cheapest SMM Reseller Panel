#block_12 .navbar-icon { margin-right: 8px; min-width: 20px; }

#block_12 .navbar-languages { padding-left: 12px; padding-right: 12px; }

#block_12 .navbar-rtl .navbar-languages { margin-left: 0px; margin-right: 12px; }

#block_12 .navbar-rtl .navbar-icon { margin-right: 0px; margin-left: 8px; }

#block_12 .navbar-rtl .component-navbar-collapse { margin-left: inherit; margin-right: auto; }

@media (max-width: 991px) {
  #block_12 .navbar-languages { margin-top: 4px; padding-left: 0px; padding-right: 0px; }
  #block_12 .navbar-rtl .navbar-languages { margin-left: 12px; margin-right: 0px; }
  #block_12 .sidebar-block__top { position: relative; }
  #block_12 .wrapper-sidebar-navbar { display: block; }
}

